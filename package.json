{
  "name": "centinel-api",
  "version": "1.0.0",
  "description": "",
  "author": "MD. ASADULLAH AL GALIB",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:seed": "ts-node dist/database/seeds/seed.js",
    "db:migration:create": "yarn typeorm migration:create",
    "db:migration:run": "yarn typeorm migration:run -d dist/database/ormconfig.js",
    "db:migration:revert": "yarn typeorm migration:revert -d dist/database/ormconfig.js",
    "db:drop": "yarn typeorm schema:drop -d dist/database/ormconfig.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/platform-socket.io": "^11.0.10",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/serve-static": "^5.0.2",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.10",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "bullmq": "^5.41.3",
    "cache-manager": "6.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csurf": "^1.11.0",
    "ejs": "3.1.10",
    "googleapis": "^144.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "kafkajs": "^2.2.4",
    "livekit-server-sdk": "^2.10.2",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.10.2",
    "otp-without-db": "^1.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.3",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^5.0.0",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.13.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "29.7.0",
    "prettier": "^3.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
